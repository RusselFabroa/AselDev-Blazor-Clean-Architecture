@using AselBlazorCleanArchitecture.Server.Components.Layout



<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="app.css" />
    <link rel="stylesheet" href="AselBlazorCleanArchitecture.Server.styles.css" />
    <link rel="icon" type="image/png" href="favicon.png" />

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap" rel="stylesheet">

    <link href="_content/MudBlazor/MudBlazor.min.css?v=@(MudBlazor.Metadata.Version)" rel="stylesheet" />
    <script src="_content/MudBlazor/MudBlazor.min.js?v=@(MudBlazor.Metadata.Version)"></script>

    <HeadOutlet />
    <style>
        html {
            font-size: clamp(9px, 1.3vw + 0.6rem, 10px);
            font-family: 'Poppins', 'Inter', 'Helvetica Neue', Helvetica, Arial, sans-serif;
        }

        * {
            font-size: clamp(9px, 1.3vw + 0.6rem, 10px);
            font-family: 'Poppins', 'Inter', 'Helvetica Neue', Helvetica, Arial, sans-serif;
        }
    </style>
</head>

<body>
    @* <Routes @rendermode="InteractiveServer">
        
    </Routes> *@
    
    <Router AppAssembly="@typeof(App).Assembly" >
        <Found Context="routeData" >
            @* <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)"/> *@
            <Routes @rendermode="InteractiveServer"></Routes>
            <FocusOnNavigate RouteData="@routeData" Selector="h1" />
        </Found>
        <NotFound>
            <LayoutView Layout="@typeof(MainLayout)">
                <p>Sorry, there's nothing at this address.</p>
            </LayoutView>
        </NotFound>
    </Router>


    <script src="_content/MudBlazor/MudBlazor.min.js?v=@(MudBlazor.Metadata.Version)"></script>
    <!-- Important: Increment the version parameter whenever you update MudBlazor to prevent caching issues -->
 

    <script src="_framework/blazor.web.js"></script>
</body>

</html>

   


